{
  "expo": {
    "name": "Ceta",
    "slug": "Ceta",
    "scheme": "Ceta",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/react.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/react.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "ios": {
      "supportsTablet": true,
      "package" : "com.hmn.aora"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package" : "com.hmn.aora"
    },
    "web": {
      "favicon": "./assets/react.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
